# OpenAI API Configuration (required)
OPENAI_API_KEY=your-api-key-here

# Optional: Output directory (default: forge)
OUTPUT_DIR=forge

# Optional: Model configuration file
MODEL_CONFIG=model_teams_config.json

# Optional: Default temperature for models
DEFAULT_TEMPERATURE=0.7

# Optional: Max retries for API calls
MAX_RETRIES=3

# Optional: Delay between API calls (seconds)
API_DELAY=0.5